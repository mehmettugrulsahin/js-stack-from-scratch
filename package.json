{
  "name": "js-stack-from-scratch",
  "version": "1.9.0",
  "description": "JavaScript Stack from Scratch - Step-by-step tutorial to build a modern JavaScript stack",
  "main": "index.js",
  "scripts": {
    "start": "gulp",
    "test": "echo \"Error: no test specified\" && exit 1",
    "tutorial-test": "yarn start"
  },
  "babel": {
    "compact": false,
    "presets": [
      "latest",
      "react"
    ]
  },
  "eslintConfig": {
    "rules": {
      "new-cap": [
        2,
        {
          "capIsNewExceptions": [
            "Map",
            "List"
          ]
        }
      ]
    },
    "env": {
      "browser": true
    },
    "extends": "airbnb",
    "plugins": [
      "import"
    ]
  },
  "repository": {
    "url": "https://github.com/mehmettugrulsahin/js-stack-from-scratch.git",
    "type": "git"
  },
  "author": "Mehmet Tuğrul Şahin <mehmettugrulsahin@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "babel-polyfill": "^6.16.0",
    "immutable": "^3.8.1",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "redux": "^3.6.0"
  },
  "devDependencies": {
    "babel-loader": "^6.2.7",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "del": "^2.2.2",
    "eslint": "^3.9.0",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-import": "^2.0.1",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.4.1",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-eslint": "^3.0.1",
    "webpack-stream": "^3.2.0"
  }
}
